{
  "extends": "@istanbuljs/nyc-config-typescript",
  "all": true,
  "check-coverage": true,
  "lines": 80,
  "functions": 85,
  "branches": 75,
  "statements": 80,
  "include": [
    "out/parsers/**/*.js",
    "out/services/**/*.js",
    "out/utils/**/*.js",
    "out/generators/**/*.js"
  ],
  "exclude": [
    "out/test/**",
    "out/**/*.test.js"
  ],
  "reporter": [
    "html",
    "text",
    "text-summary"
  ],
  "report-dir": "./coverage"
}